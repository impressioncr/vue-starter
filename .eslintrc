{
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "standard"
  ],
  "plugins": [
    "html",
    "vue"
  ],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "no-new": "off"
  },
  "globals": {
    "MEM_ENV": true
  }
}