{
  "name": "webpck5-vue",
  "version": "1.0.0",
  "description": "vue starter",
  "main": "index.js",
  "repository": "https://github.com/lushdog/vue-starter",
  "author": "lushdog",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=DEV webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "dev": "nodemon --watch build --exec \"cross-env NODE_ENV=DEV webpack-dev-server --inline --config build/webpack.dev.conf.js\"",
    "build": "rimraf dist && cross-env NODE_ENV=PRD webpack --config build/webpack.prod.conf.js",
    "build:dll": "webpack --config build/webpack.dll.conf.js",
    "fix": "eslint --ext .vue --ext .js src/ --fix"
  },
  "browserslist": [
    "iOS >= 8",
    "android >= 4.2"
  ],
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "core-js": "3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.0.4",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-px2rem": "^0.3.0",
    "progress": "^2.0.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^4.0.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
